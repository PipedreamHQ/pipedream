{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Next.js: Full Stack",
      "runtimeExecutable": "node",
      "runtimeArgs": [
        "--inspect",
        "./node_modules/.bin/next",
        "dev"
      ],
      "attachSimplePort": 9230,
      "skipFiles": ["<node_internals>/**"],
      "sourceMaps": true,
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}
